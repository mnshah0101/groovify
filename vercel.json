{
    "version": 2,
    "name": "Groovify",
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js"
        }
    ],
    "env": {
        "CUSTOM_ENV_VARIABLE": "my-value"
    },
    "regions": [
        "sfo1"
    ]
}